{
  "name": "posthtml-vscode-sorting-attrs-logic",
  "description": "It's sort HTML attribute based on the given order",
  "version": "1.0.5",
  "author": "RaphaÃ«l Balet (ultrastark.ch)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rbalet/posthtml-vscode-sorting-attrs.git"
  },
  "license": "MIT",
  "keywords": [
    "posthtml",
    "html",
    "posthtml-plugin",
    "attrs",
    "attributes",
    "sorter",
    "sort"
  ],
  "dependencies": {},
  "devDependencies": {
    "mocha": "^5.2.0",
    "xo": "^0.23.0"
  },
  "engines": {
    "node": ">= 0.12.0"
  },
  "scripts": {
    "test": "xo && mocha"
  },
  "files": [
    "index.js"
  ],
  "xo": {
    "rules": {
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "arrow-parens": [
        2,
        "always"
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "babel/object-curly-spacing": 0,
      "space-before-function-paren": [
        2,
        "never"
      ],
      "generator-star-spacing": [
        2,
        "after"
      ]
    },
    "envs": [
      "node",
      "mocha"
    ]
  },
  "bugs": {
    "url": "https://github.com/rbalet/posthtml-vscode-sorting-attrs/issues"
  },
  "homepage": "https://github.com/rbalet/posthtml-vscode-sorting-attrs#readme",
  "main": "index.js"
}
